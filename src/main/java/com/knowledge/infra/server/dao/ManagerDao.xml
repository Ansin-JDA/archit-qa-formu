<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.knowledge.infra.server.dao.UserDao">  

<insert id="addManager" parameterType="com.knowledge.infra.server.model.Manager" flushCache="true">  
   INSERT INTO t_user (mname,memail,mpassword) VALUES (#{mname},#{memail},#{mpassword})  
</insert>  
<update id="updateManager" parameterType="com.knowledge.infra.server.model.Manager">  
    UPDATE user SET mname=#{mname},memail=#{memail}, mpassword=#{mpassword} mstatus = #{mstatus}WHERE id=#{managerid}  
</update>  
<delete id="deleteManager" parameterType="com.knowledge.infra.server.model.Manager">  
    DELETE FROM user WHERE managerid=#{managerid}  
</delete>  
</mapper>